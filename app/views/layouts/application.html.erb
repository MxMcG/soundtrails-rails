<!DOCTYPE html>
<html>
<head>
  <title>SoundtrailsRails</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAC5iWnfUdbm2J5zFBGFMQyDYiDoYR1XXI"
  type="text/javascript"></script>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
    <% if user_signed_in? %>
      <li>
      <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>
      </li>
      <li>
      <%= link_to('Community', users_path()) %>
      </li>
      <li>
      <%= link_to('Profile', profiles_path(current_user)) %>
      </li>
      <li>
      <%= link_to('Maps', new_user_map_path(current_user)) %>
      </li>
    <% end %>
  </nav>
<%= yield %>

</body>
</html>
